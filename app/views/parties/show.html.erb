<%= render 'shared/partydescription' %>

<% if @party.user != current_user %>

  <% if current_user.bookings.index{ |x| @party.bookings.include?(x)}.nil? %>
    <%= render 'shared/bookingform'  %>

  <% else %>
    <%= render 'shared/bookingreview' %>

  <% end %>

<% else %>

  <%= render 'shared/bookingslist' %>

<% end %>






















